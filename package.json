{
	"name": "@carnesen/mathjs-app",
	"description": "A toy calculator web app built on math.js",
	"version": "2022.2.1",
	"main": "index.js",
	"scripts": {
		"build:browser": "cross-env BABEL_ENV=browser webpack",
		"build:browser:min": "cross-env NODE_ENV=production npm run build:browser -- --bail",
		"build:browser:watch": "npm run build:browser -- --watch",
		"build:server": "cross-env BABEL_ENV=server babel src --out-dir lib --ignore *-test.js,browser",
		"build:server:watch": "npm run build:server -- --watch",
		"build": "npm run clean && npm run build:server && npm run build:browser:min",
		"clean": "rimraf lib dist",
		"lint": "eslint src webpack.config.babel.js",
		"lint:fix": "npm run lint -- --fix",
		"release": "carnesen-dev release --semverBump date",
		"start": "node server.js",
		"start:watch": "nodemon --watch lib server.js",
		"test": "npm run lint && npm run build && npm run test:server && npm run test:browser",
		"test:browser": "cross-env BABEL_ENV=browser NODE_ENV=test mocha --require .before-browser-tests.js src/browser/**/*-test.js src/shared/**/*-test.js",
		"test:server": "cross-env BABEL_ENV=server NODE_ENV=test mocha --require .before-server-tests.js src/server/**/*-test.js src/shared/**/*-test.js"
	},
	"dependencies": {
		"@carnesen/checks": "0.0.2",
		"@carnesen/logger": "0.1.2",
		"@carnesen/redux-add-action-listener-enhancer": "0.0.1",
		"@carnesen/redux-sliced": "0.0.3",
		"@carnesen/util": "0.0.8",
		"@carnesen/web-server": "0.0.1",
		"cross-env": "3.1.3",
		"http-errors": "1.5.0",
		"mathjs": "3.17.0",
		"octicons": "5.0.1",
		"statuses": "1.3.0",
		"uuid": "2.0.3"
	},
	"devDependencies": {
		"@carnesen/dev": "0.1.1",
		"@carnesen/styles": "0.0.1",
		"babel-cli": "6.18.0",
		"babel-core": "6.18.2",
		"babel-loader": "6.2.7",
		"babel-plugin-transform-async-to-generator": "6.16.0",
		"babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
		"babel-polyfill": "6.16.0",
		"babel-preset-es2015": "6.18.0",
		"babel-preset-react": "6.16.0",
		"babel-register": "6.18.0",
		"chai": "3.5.0",
		"css-loader": "0.25.0",
		"enzyme": "2.5.1",
		"eslint": "3.19.0",
		"eslint-config-carnesen": "1.0.5",
		"file-loader": "0.9.0",
		"json-loader": "0.5.4",
		"material-ui": "0.16.1",
		"mocha": "3.1.2",
		"nodemon": "1.11.0",
		"react": "15.3.2",
		"react-addons-test-utils": "15.3.2",
		"react-dom": "15.3.2",
		"react-redux": "4.4.5",
		"react-tap-event-plugin": "1.0.0",
		"rimraf": "2.5.4",
		"roboto-fontface": "0.6.0",
		"socket.io-client": "1.5.1",
		"style-loader": "0.13.1",
		"supertest": "2.0.1",
		"supertest-as-promised": "4.0.2",
		"webpack": "1.13.3",
		"whatwg-fetch": "1.0.0"
	},
	"repository": "github:carnesen/mathjs-app",
	"author": "Chris Arnesen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carnesen/mathjs-app/issues"
	},
	"homepage": "https://github.com/carnesen/mathjs-app#readme"
}
